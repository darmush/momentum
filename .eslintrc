{
    "parser": "@babel/eslint-parser",
    "rules": {
      "no-unused-vars": "warn"
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "root": true,
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "requireConfigFile": false,
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true,
            "modules": false
          }
        ]
      ]
    }
}
